{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@builder.io/qwik",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"],
      "@qwik-client-manifest": ["./dist/qwik-client-manifest.json"],
      "@qwik-city-plan": ["./dist/qwik-city-plan.json"]
    }
  },
  "include": ["src"]
}